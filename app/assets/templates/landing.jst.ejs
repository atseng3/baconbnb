<div class="jumbotron">
		<form role="search">
			<div class="main-search">
				<h1>Find a place to stay.</h1><br>
				<h4>Rent from people in over 34,000 cities and 192 countries.</h4><br>
				<input 
					type="text" 
					name="search[location]" 
					placeholder="Where do you want to go?">
		
				<input 
					type="date" 
					name="search[check_in]"
					placeholder="Check in">
		
				<input 
					type="date" 
					name="search[check_out]"
					placeholder="Check out">
		
				<input 
					type="number" 
					name="search[num_guests]"
					placeholder="1 Guest">

				<button 
					type="submit" 
					class="btn btn-default">Search</button>
			</div>
		</form>
</div>


<div class="neighborhoods">
<div class="sub-neighborhoods">San Francisco</div>
<div class="sub-neighborhoods">New York</div>
<div class="sub-neighborhoods">Tokyo</div>
<div class="sub-neighborhoods">Sydney</div>
<div class="sub-neighborhoods">Taipei</div>
<div class="sub-neighborhoods">London</div>
</div>